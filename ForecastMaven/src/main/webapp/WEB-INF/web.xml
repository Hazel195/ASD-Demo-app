<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>MapLocation_Servlet</servlet-name>
        <servlet-class>Controller.MapLocation_Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CurrentWeather_Servlet</servlet-name>
        <servlet-class>Controller.CurrentWeather_Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditAccountAdmin</servlet-name>
        <servlet-class>Controller.EditAccountAdmin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteAccountAdmin</servlet-name>
        <servlet-class>Controller.DeleteAccountAdmin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MapLocation_Servlet</servlet-name>
        <url-pattern>/MapLocation_Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CurrentWeather_Servlet</servlet-name>
        <url-pattern>/CurrentWeather_Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditAccountAdmin</servlet-name>
        <url-pattern>/EditAccountAdmin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteAccountAdmin</servlet-name>
        <url-pattern>/DeleteAccountAdmin</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
