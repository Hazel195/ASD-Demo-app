<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>MapLocation_Servlet</servlet-name>
        <servlet-class>Controller.MapLocation_Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CurrentWeather_Servlet</servlet-name>
        <servlet-class>Controller.CurrentWeather_Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditAccountAdmin</servlet-name>
        <servlet-class>Controller.EditAccountAdmin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteAccountAdmin</servlet-name>
        <servlet-class>Controller.DeleteAccountAdmin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>WeatherHistory_Servlet</servlet-name>
        <servlet-class>Controller.WeatherHistory_Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FirstWeatherHistory_Servlet</servlet-name>
        <servlet-class>Controller.FirstWeatherHistory_Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>WeatherAnalysis_Servlet</servlet-name>
        <servlet-class>Controller.WeatherAnalysis_Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>WeatherAlert_Servlet</servlet-name>
        <servlet-class>Controller.WeatherAlert_Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FindPasswordServlet</servlet-name>
        <servlet-class>Controller.FindPasswordServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FirstWeatherForecast_Servlet</servlet-name>
        <servlet-class>Controller.FirstWeatherForecast_Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminFindPasswordServlet</servlet-name>
        <servlet-class>Controller.AdminFindPasswordServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MapLocation_Servlet</servlet-name>
        <url-pattern>/MapLocation_Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CurrentWeather_Servlet</servlet-name>
        <url-pattern>/CurrentWeather_Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditAccountAdmin</servlet-name>
        <url-pattern>/EditAccountAdmin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteAccountAdmin</servlet-name>
        <url-pattern>/DeleteAccountAdmin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>WeatherHistory_Servlet</servlet-name>
        <url-pattern>/WeatherHistory_Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FirstWeatherHistory_Servlet</servlet-name>
        <url-pattern>/FirstWeatherHistory_Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>WeatherAnalysis_Servlet</servlet-name>
        <url-pattern>/WeatherAnalysis_Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>WeatherAlert_Servlet</servlet-name>
        <url-pattern>/WeatherAlert_Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FindPasswordServlet</servlet-name>
        <url-pattern>/FindPasswordServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FirstWeatherForecast_Servlet</servlet-name>
        <url-pattern>/FirstWeatherForecast_Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminFindPasswordServlet</servlet-name>
        <url-pattern>/AdminFindPasswordServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
